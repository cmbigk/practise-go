package main

func RepeatAlpha(s string) string {
	result := ""

	for i := 0; i < len(s); i++ {
		ch := s[i]

		if ch >= 'a' && ch <= 'z' {
			repeats := int(ch - 'a' + 1) // Get the alphabetical index for lowercase
			for j := 0; j < repeats; j++ {
				result += string(ch) // Append the character `repeats` times
			}
		} else if ch >= 'A' && ch <= 'Z' {
			repeats := int(ch - 'A' + 1) // Get the alphabetical index for uppercase
			for j := 0; j < repeats; j++ {
				result += string(ch) // Append the character `repeats` times
			}
		} else {
			result += string(ch) // Non-alphabetical characters remain unchanged
		}
	}

	return result
}
